class Alumno ():

    def __init__ (self, nombre_p, nota_p):

        self.nombre = nombre_p
        self.nota = nota_p

    def atributos (self):
        print ('Almuno: {} \nNota: {}'.format(self.nombre, self.nota))

    def aprobacion (self):
        if self.nota > 6:
            print(self.nombre, 'Aprobó la materia')
        else:
            print (self.nombre,'No aprobó la materia')


Juan = Alumno('Juan', 8)
Juan.aprobacion()
